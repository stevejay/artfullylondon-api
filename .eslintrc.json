{
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-param-reassign": [
      "error",
      { "props": true, "ignorePropertyModificationsFor": ["ctx"] }
    ]
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  }
}
