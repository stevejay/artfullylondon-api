{
  "name": "artfullylondon-api-tag-service",
  "license": "MIT",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest ./src",
    "posttest": "yarn run lint",
    "test:integration": "cross-env NODE_ENV=test jest --runInBand --no-cache ./tests/integration",
    "lint": "eslint .",
    "deploy": "yarn test && sls deploy -v",
    "offline": "sls offline start --stage=development --port 3017 --noTimeout --noAuth",
    "debug:windows": "SET SLS_DEBUG=* && node --inspect ./node_modules/serverless/bin/serverless offline start --port 3017 --stage=development --noTimeout --noAuth"
  },
  "devDependencies": {
    "aws-sdk": "^2.263.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-source-map-support": "^2.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "deep-freeze-strict": "^1.1.1",
    "eslint": "^5.0.0",
    "jest": "^23.1.0",
    "jest-toolkit": "^0.1.2",
    "jsonwebtoken": "^8.3.0",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "serverless": "^1.27.3",
    "serverless-offline": "^3.25.4",
    "serverless-plugin-tracing": "^2.0.0",
    "serverless-pseudo-parameters": "^1.6.0",
    "serverless-webpack": "^5.1.5",
    "webpack": "^4.12.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "apollo-server-lambda": "^1.3.6",
    "aws-xray-sdk": "^1.2.0",
    "lodash": "^4.17.10",
    "source-map-support": "^0.5.6"
  }
}
