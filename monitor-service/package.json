{
  "name": "artfullylondon-api-monitor-service",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest ./src",
    "posttest": "yarn run lint",
    "test:integration": "cross-env NODE_ENV=test jest ./tests/integration",
    "lint": "eslint .",
    "deploy": "cross-env AWS_CLIENT_TIMEOUT=1800000 yarn test && node build.js && sls deploy --stage=production -v",
    "local:service": "cross-env sls offline start --stage=development --port 3020 --noTimeout",
    "local:install": "sls dynamodb install",
    "local:dynamodb": "node ./scripts/start-dynamodb-local.js"
  },
  "devDependencies": {
    "aws-sdk": "^2.247.1",
    "chmod": "^0.2.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "jest": "^23.0.1",
    "on-build-webpack": "^0.1.0",
    "phantomjs-prebuilt": "^2.1.16",
    "serverless": "^1.27.3",
    "serverless-webpack": "^5.1.5",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "aws-sns-publish": "^3.2.0",
    "cheerio": "^1.0.0-rc.2",
    "collapse-white-space": "^1.0.4",
    "delay": "^3.0.0",
    "dynamodb-doc-client-wrapper": "^2.0.1",
    "elasticsearch": "^15.0.0",
    "ensure-request": "^4.0.0",
    "es-search-builder": "^1.3.1",
    "fast-diff": "^1.1.2",
    "html-to-text": "^4.0.0",
    "interactive": "^0.1.9",
    "lambda-error-handler": "^1.0.0",
    "light-http": "^1.4.9",
    "lodash.chunk": "^4.2.0",
    "lodash.some": "^4.6.0",
    "lodash.uniq": "^4.5.0",
    "lodash.zip": "^4.2.0",
    "loglevel": "^1.6.1",
    "node-horseman": "^3.3.0",
    "qsm": "^2.0.2",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "sanitize-html": "^1.18.2",
    "serverless-dynamodb-client": "^0.0.2"
  }
}
